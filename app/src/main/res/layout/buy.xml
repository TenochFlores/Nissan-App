<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BuyFragment">

    <TextView
        android:id="@+id/registro_etiqueta_login3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Compras en espera"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/home_etiqueta_correo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="140dp"
        android:text="ALTIMA:"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_etiqueta_rfc"
        app:layout_constraintVertical_bias="0.489" />

    <TextView
        android:id="@+id/home_etiqueta_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="78dp"
        android:text="NISSAN Z:"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_etiqueta_correo"
        app:layout_constraintVertical_bias="0.418" />

    <TextView
        android:id="@+id/home_etiqueta_password3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="28dp"
        android:text="NISSAN GTR:"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_etiqueta_password" />

    <TextView
        android:id="@+id/buy_nissan_z"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="78dp"
        android:text="@string/etiqueta_password"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/home_etiqueta_password"
        app:layout_constraintTop_toTopOf="@+id/home_etiqueta_password"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/buy_nissan_gtr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/etiqueta_password"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/home_etiqueta_password3"
        app:layout_constraintTop_toTopOf="@+id/home_etiqueta_password3" />

    <TextView
        android:id="@+id/buy_altima"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="78dp"
        android:text="@string/etiqueta_password"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/home_etiqueta_correo"
        app:layout_constraintTop_toTopOf="@+id/home_etiqueta_correo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/buy_versa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="78dp"
        android:text="@string/etiqueta_password"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/home_etiqueta_nombre"
        app:layout_constraintTop_toTopOf="@+id/home_etiqueta_nombre"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/buy_sentra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="78dp"
        android:text="@string/etiqueta_password"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/home_etiqueta_rfc"
        app:layout_constraintTop_toTopOf="@+id/home_etiqueta_rfc"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/home_etiqueta_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="260dp"
        android:text="VERSA:"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_etiqueta_datos" />

    <TextView
        android:id="@+id/home_etiqueta_rfc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="196dp"
        android:text="SENTRA:"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_etiqueta_nombre"
        app:layout_constraintVertical_bias="0.479" />

    <TextView
        android:id="@+id/home_etiqueta_datos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="332dp"
        android:text="Cantidad por modelo:"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="331dp"
        android:layout_height="256dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registro_etiqueta_login3"
        app:srcCompat="@drawable/nissan_modelos" />

</androidx.constraintlayout.widget.ConstraintLayout>